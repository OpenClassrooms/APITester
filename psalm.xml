<?xml version="1.0"?>
<psalm xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"

       errorLevel="1"
       usePhpDocMethodsWithoutMagicCall="true"
       usePhpDocPropertiesWithoutMagicCall="true"
       checkForThrowsDocblock="true"
       memoizeMethodCallResults="true"
       addParamDefaultToDocblockType="true"
       findUnusedVariablesAndParams="true"
       sealAllMethods="true"

       xmlns="https://getpsalm.org/schema/config"
       xsi:schemaLocation="https://getpsalm.org/schema/config vendor/vimeo/psalm/config.xsd">
    <projectFiles>
        <directory name="src"/>
        <directory name="tests"/>
        <ignoreFiles>
            <directory name="vendor"/>
        </ignoreFiles>
    </projectFiles>
    <plugins>
        <pluginClass class="Psalm\PhpUnitPlugin\Plugin"/>
        <pluginClass class="Psl\Psalm\Plugin"/>
        <pluginClass class="Orklah\PsalmInsaneComparison\Plugin"/>
        <!--        <pluginClass class="Orklah\StrictTypes\Plugin"/>-->
        <pluginClass class="Orklah\StrictEquality\Plugin"/>
        <!--        <pluginClass class="Orklah\UseConstants\Plugin"/>-->
        <pluginClass class="AlexS\iter\PsalmPlugin"/>
        <!--        <pluginClass class="Sfp\Psalm\TypedLocalVariablePlugin\Plugin"/>-->
        <pluginClass class="Psl\Integration\Psalm\Plugin"/>
        <pluginClass class="Orklah\TypeSetters\Plugin"/>
        <pluginClass class="Orklah\ElvisBegone\Plugin"/>
    </plugins>
    <issueHandlers>
    </issueHandlers>
</psalm>
