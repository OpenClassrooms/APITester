{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "cebe/php-openapi": "1.4.*",
        "n98/junit-xml": "1.*",
        "nelmio/alice": "3.*",
        "nesbot/carbon": "2.*",
        "nyholm/psr7": "1.*",
        "psr/http-client": "^1.0",
        "psr/http-message": "1.*",
        "symfony/console": "5.*",
        "symfony/http-client": "5.*",
        "symfony/psr-http-message-bridge": "*",
        "webmozart/assert": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.*",
        "symfony/var-dumper": "^5.2",
        "symfony/yaml": "^5.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "OpenAPITesting\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenAPITesting\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    }
}
