{
  "name": "openclassrooms/open-api-testing",
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.4",
    "cebe/php-openapi": "1.4.*",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "n98/junit-xml": "1.*",
    "nelmio/alice": "3.*",
    "nesbot/carbon": "2.*",
    "nyholm/psr7": "1.*",
    "psr/http-message": "1.*",
    "psr/http-client": "1.*",
    "symfony/console": "5.*",
    "symfony/psr-http-message-bridge": "*",
    "symfony/mime": "5.*",
    "webmozart/assert": "1.*",
    "sensio/framework-extra-bundle": "^5.6",
    "symfony/http-client": "5.*",
    "symfony/dotenv": "5.*",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "5.*",
    "symfony/twig-bundle": "5.*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.*"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "OpenAPITesting\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OpenAPITesting\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.2.*"
    }
  }
}
